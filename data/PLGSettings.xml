<?xml version="1.0" encoding="ISO-8859-1"?>
<plgsettings>
  <!-- Relative path to a BibTeX file with the publication list -->
  <publications>testPub.bib</publications>

  <!-- Settings for plain-text output -->
  <generateplaintext>false</generateplaintext>
  <plaintextsettings>
    <target>publications.txt</target>
    <listallauthors>true</listallauthors>
    <presentedtext><![CDATA[null]]></presentedtext>
    <numbering>NONE</numbering>
    <categories>
    </categories>
    <categorynotes>
    </categorynotes>
  </plaintextsettings>

  <!-- Settings for HTML output -->
  <generatehtml>true</generatehtml>
  <htmlsettings>
    <target>testPub.html</target>
    <listallauthors>true</listallauthors>
    <presentedtext><![CDATA[<img src="fubar.png">]]></presentedtext>
    <numbering>NONE</numbering>
    <categories>
      <category>JOURNAL</category>
      <category>CONFERENCE</category>
    </categories>
    <categorynotes>
      <note category="CONFERENCE"><![CDATA[I have a line-end here:<br>]]></note>
    </categorynotes>
    <!-- HTML-specific settings -->
    <linktotextversion>false</linktotextversion>
    <includeabstract>ALL</includeabstract>
    <includebibtex>ARXIV</includebibtex>
    <includepdf>ARXIV</includepdf>
    <header>data\defaultHeader.html</header>
    <footer>data\defaultFooter.html</footer>
    <googleanalyticsuser><![CDATA[null]]></googleanalyticsuser>
  </htmlsettings>

</plgsettings>
